{
  "$schema": "http://json.schemastore.org/proxies",
  "proxies": {
    "proxy1": {
      "matchCondition": {
          "methods": [ "POST" ],
          "route": "/CreateRating"
      },
      "backendUri": "https://koyu-api-managament.azure-api.net/koyu-production"
    },
    "proxy2": {
      "matchCondition": {
          "methods": [ "GET" ],
          "route": "/GetRating"
      },
      "backendUri": "https://koyu-api-managament.azure-api.net/koyu-production"
    }
  }
}
